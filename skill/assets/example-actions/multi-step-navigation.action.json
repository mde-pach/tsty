{
  "type": "navigation",
  "description": "Navigate through multiple pages",
  "primitives": [
    {
      "type": "navigate",
      "url": "${baseUrl}/dashboard"
    },
    {
      "type": "wait",
      "condition": "networkidle"
    },
    {
      "type": "click",
      "selector": "a:has-text('Settings'), nav a[href*='settings']"
    },
    {
      "type": "wait",
      "condition": "networkidle"
    },
    {
      "type": "click",
      "selector": "a:has-text('Profile'), button:has-text('Profile')"
    },
    {
      "type": "wait",
      "condition": "networkidle"
    }
  ]
}
